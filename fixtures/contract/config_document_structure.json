{
  "id": "config_document_structure",
  "description": "Tests include_document_structure config produces document tree",
  "tags": ["contract", "document_structure"],
  "document": { "path": "pdf/fake_memo.pdf" },
  "extraction": {
    "method": "sync",
    "input_type": "file",
    "config": { "include_document_structure": true }
  },
  "assertions": {
    "expected_mime": "application/pdf",
    "document": {
      "has_document": true,
      "min_node_count": 1,
      "node_types_include": ["paragraph"]
    }
  }
}
