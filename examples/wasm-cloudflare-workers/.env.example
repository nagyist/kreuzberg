# Cloudflare Configuration
# Copy this file to .env and fill in your values

# Cloudflare Account ID (found in dashboard)
CLOUDFLARE_ACCOUNT_ID=

# Cloudflare API Token (create in dashboard)
CLOUDFLARE_API_TOKEN=

# Zone ID for your domain (if deploying to a specific zone)
CLOUDFLARE_ZONE_ID=

# Production domain (if deploying to production)
PRODUCTION_DOMAIN=api.yourdomain.com

# Development settings
DEBUG=false
LOG_LEVEL=info
